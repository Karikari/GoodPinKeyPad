<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GoodPinKeyPad">

        <attr name="backgroundColor" format="color" />

        <attr name="keyPadTextColor" format="color" />

        <attr name="keyPadTextSize" format="string"/>

        <attr name="keyPadBackPressIcon" format="integer"/>

        <attr name="keyPadCancelAllIcon" format="integer"/>

        <attr name="pinEntry" format="enum">
            <enum name="four" value="4"/>
            <enum name="five" value="5"/>
            <enum name="six" value="6"/>
        </attr>

        <attr name="KeyPadTheme" format="enum">
            <enum name="white" value="1"/>
            <enum name="dark" value="2"/>
            <enum name="white_border" value="3"/>
            <enum name="dark_border" value="4"/>
        </attr>

    </declare-styleable>
</resources>